	<queryString>
		<![CDATA[SELECT
        upper(ISNULL(PO.centroestudio,'')) AS institucion,
        concat(convert(varchar(2),right(replicate('0', 1) + rtrim(isnull(PO.mesini,0)), 2)),'/',convert(varchar(4),isnull(pO.anoini,'')) ,' - ' ,convert(varchar(2),right(replicate('0', 1) + rtrim(isnull(PO.mesfin,0)), 2)),'/',convert(varchar(4),isnull(PO.anofin,''))) as periodo,
        upper(ISNULL(PO.curso,'')) AS cursoSem,
        (case when po.nrohoras >9 then right(replicate('0', 1) + rtrim(isnull(PO.nrocreditos,0)), 2) else convert(varchar,PO.nrocreditos) end) as nroCreditos,
        (case when po.nrohoras >9 then right(replicate('0', 1) + rtrim(isnull(PO.nrohoras,0)), 2) else convert(varchar,PO.nrohoras) end) as nroHoras
        FROM MO_PersonalOtrosEstudios PO
        order by 2]]>
	</queryString>
