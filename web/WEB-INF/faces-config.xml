            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/panControl.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>toRootMenu</from-outcome>
            <to-view-id>/menuControl.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
